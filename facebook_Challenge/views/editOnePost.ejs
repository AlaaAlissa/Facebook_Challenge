<%- include('./components/header')%>

    <%- include('./components/navbar')%>

    <form method="POST" action=/feed/edit/<%= result._id %>>
        <div class="form-group">
          <label for="exampleInputEmail1">Name</label>
          <input type="text" name="Name" value= "<%= result.Name %>" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
          <% if( error && error.Name){%> 
            <small id="emailHelp" class="form-text text-muted"><%= error.Name.properties.message %>  <i class="Medium material-icons">error</i></small>
            <%}%>
        </div>
        <div class="form-group">
          <label for="exampleInputPassword1">Message</label>
          <input type="text" name="Message" value= "<%= result.Message %>" class="form-control" id="exampleInputPassword1">
          <% if( error && error.Message){%> 
            <small id="emailHelp" class="form-text text-muted"><%= error.Message.properties.message %>  <i class="Medium material-icons">error</i></small>
            <%}%>
        </div>
    
        <button type="submit" class="btn btn-success">Submit</button>
      </form>

<%- include('./components/footer')%>