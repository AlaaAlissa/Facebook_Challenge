<%- include('./components/header')%>

    <%- include('./components/navbar')%>

    <% if(result){%>            
  <div class="row">
    <div class="col s12 m6" >
      <div class="card blue-grey darken-1" id="cards" >
        <div class="card-content white-text" >
          <span class="card-title"> <%= result.Name %></span>
          <p><%= result.Message %></p>
        </div>
        <div class="card-action">
          <a href=/feed/edit/<%= result._id%>>Update</a>
          <a href=/delete-post/<%= result._id%>>Delete</a>
        </div>
        <p id="createddate"><%= result.createdAt %></p>
      </div>
    </div>
  </div>
        <%} else {%> 
            there is no record
            <%}%>


